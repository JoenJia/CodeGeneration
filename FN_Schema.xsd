<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2017 rel. 3 sp1 (http://www.altova.com) by First National Financial (First National Financial) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Deal">
		<xs:annotation>
			<xs:documentation>The loan/deal submission</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DealInfo">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Version" type="xs:string"/>
							<xs:element name="DealType" type="XSEnumCodeType">
								<xs:annotation>
									<xs:documentation>Enum: 
0 Approval, 
1 Pre-Approval</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ApplicationDate" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>The date deal was initially submitted to any lender.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SubmissionDate" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>The date deal submitted to FN.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Purpose" type="XSEnumCodeType">
								<xs:annotation>
									<xs:documentation>
0 - Purchase
1 - Purcahse + Improvements
2 - Refinance
4 - Switch
5 - Port</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ImprovementFlag" type="xs:int" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Indicates if improvements being done on a purchase:
0 - No
1 - Yes</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ImprovementsAmount" type="xs:decimal" minOccurs="0"/>
							<xs:element name="ImprovementsDesc" type="xs:string" minOccurs="0"/>
							<xs:element name="FinancingWaiverDate" type="xs:string" minOccurs="0"/>
							<xs:element name="GDS" type="xs:decimal" minOccurs="0"/>
							<xs:element name="TDS" type="xs:decimal" minOccurs="0"/>
							<xs:element name="LTV" type="xs:decimal" minOccurs="0"/>
							<xs:element name="BusinessSource" type="XSEnumCodeType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Enum:
0 Broker
</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Applicant" type="XSApplicantType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>List of applicants</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RequestedLoan" type="XSRequestedLoanType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>The mortgage/heloc loan details</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SubjectProperty">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Property" type="XSSubjectPropertyType">
								<xs:annotation>
									<xs:documentation>The subject property on which the loan is requested/secured.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ExistingLoan" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>ToDo: Business to review how much information we need to capture about a mortgage on an existing property.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="XSExistingLoanType"/>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Brokerage">
					<xs:annotation>
						<xs:documentation>Details about the broker/agent/firm submitting the deal.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Broker" maxOccurs="2">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Firm" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ID">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="100"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="Name">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:maxLength value="200"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="LicenseRegistrationNumber" type="xs:string" minOccurs="0"/>
													<xs:element name="Contact" type="XSContactMethodsType" minOccurs="0"/>
													<xs:element name="Address" type="XSAddressType" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="ID">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="100"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="BrokerType" type="XSEnumCodeType">
											<xs:annotation>
												<xs:documentation>Enum specifying submitting agent, broker of record etc.
1 - Originator
2 - Reviewer</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="LicenseRegistrationNumber" type="xs:string" minOccurs="0"/>
										<xs:element name="FirstName">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="50"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="MiddleName" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="30"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="LastName">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="50"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="Contact" type="XSContactMethodsType" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="System">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CompanyId">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="100"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SubmissionId">
								<xs:annotation>
									<xs:documentation>Unique ID for any submission, same deal first submission and re-submission  will have different submissionId</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="50"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SenderReferenceId">
								<xs:annotation>
									<xs:documentation>external system deal unique id</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="100"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SubmissionNumber" type="xs:int" minOccurs="0"/>
							<xs:element name="RevisionNumber" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Holds a refernce to the senders schema version/revision.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="50"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Party" type="XSPartyType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>3rd parties involved who are not submitting agent/broker, e.g. Solicitors etc.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Comment" type="XSCommentType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="XSAddressType">
		<xs:annotation>
			<xs:documentation>full address</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Unit" minOccurs="0">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetNumber">
				<xs:annotation>
					<xs:documentation>the number on the street.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetName">
				<xs:annotation>
					<xs:documentation>Identifies the name of the street.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetType">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetDirection" type="XSEnumCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>1 N
2 S
3 E
4 W
5 NE
6 NW
7 SE
8 SW</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ProvinceCode">
				<xs:annotation>
					<xs:documentation>Provinces / States code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PostalCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>standard 2 characters province or states code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CountryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Standard 3 charcaters country code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="XSEnumCodeType">
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:complexType name="XSContactMethodsType">
		<xs:sequence>
			<xs:element name="Telephone" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TelType" type="XSEnumCodeType">
							<xs:annotation>
								<xs:documentation>0 - Home Phone
1 - Cell Phone
2 - Fax
4 - Work Phone</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TelNumber">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Extension" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Email" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="EmailAddress">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XSCommentType">
		<xs:sequence>
			<xs:element name="Content">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8192"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CommentType" type="XSEnumCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Enum: 
0 - System
1 - Broker
2 - Solicitor
3 - Underwriter
4 - Lender
99 - Other</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Title" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Title/Subject</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CommentDate" type="xs:dateTime"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XSIncomeType">
		<xs:sequence>
			<xs:element name="ID" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IncomeType" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>Enum: 
0 - Salary
1 - Hourly
10 - Other
11 - Other Employment Income
2 - Hourly + Commissions
3 - Commissions
4 - Interest Income
5 - Child Support
6 - Self-Employed
7 - Pension
8 - Alimony
9 - Rental Income</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Frequency" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>
0 Annual
1 Semi-Annual
2 Quarterly
3 Monthly
4 Semi-Monthly
5 Bi-Weekly
6 Weekly</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount" type="xs:decimal" default="0"/>
			<xs:element name="PrimaryIncome" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="XSSubjectPropertyType">
		<xs:complexContent>
			<xs:extension base="XSPropertyBaseType">
				<xs:sequence>
					<xs:element name="DwelingType" type="XSEnumCodeType">
						<xs:annotation>
							<xs:documentation>Enum: 
1 - Deteched
2 - Semi
3 - Duplex
4 - Row
5 - Apartment
6 - Mobile
8 - Tri Plex
9 - Other
10 - Stacked
17 - Modular
18 - Four Plex</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AppraisedValue" type="xs:decimal"/>
					<xs:element name="PurchasePrice" type="xs:decimal"/>
					<xs:element name="OriginalPurchasePrice" type="xs:decimal" minOccurs="0"/>
					<xs:element name="PurchaseDate" type="xs:date" minOccurs="0"/>
					<xs:element name="Style" type="XSEnumCodeType">
						<xs:annotation>
							<xs:documentation>Enum: 
1 - One Storey
2 - Bi - Level
3 - Two Storey
4 - Split Level
5 - 1 1/2 Storey
6 - 3 Storey
99 - Other</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DwelingSize" type="XSSizeType"/>
					<xs:element name="YearConstructed" type="xs:int"/>
					<xs:element name="Units" type="xs:int"/>
					<xs:element name="Tenure" type="XSEnumCodeType">
						<xs:annotation>
							<xs:documentation>Emum:
0 - Freehold
1 - Leasehold
2 - Condominium</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HeatSource" type="XSEnumCodeType">
						<xs:annotation>
							<xs:documentation>Enum:
1 - Electric Baseboard
2 - Forced Air Gas/Oil/Electric
3 - Hot Water
4 - Other</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WaterSupply" type="XSEnumCodeType">
						<xs:annotation>
							<xs:documentation>Enum:
0 - Uknown
1 - Municipal
2 - Co-Operative
3 - Drilled Well
4 - Dug Well
5 - Share Well
6 - Community Well
7 - Lake Intake
8 - None
9 - Other</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Sewer" type="XSEnumCodeType">
						<xs:annotation>
							<xs:documentation>Enum:
0 - Uknown
1 - Sewer Connected
2 - Sewer Not Connected
3 - Septic Installed
4 - Septic Approved
5 - None</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Zoning" type="XSEnumCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Enum: 1-Major Urban Residential, 
2 Urban Residential, 
3 Rural Residential, 
4 Agricultural, 
5 Agricultural land reserve, 
6 Recreational / Cottage, 
7 Commercial, 
8 Commercial / Residential, 
99 Other</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ListingType" type="XSEnumCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
0 - MLS
1 - Exclusive
2 - Private</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MLSNumber" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="50"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="TaxRollNumber" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="XSRequestedLoanType">
		<xs:sequence>
			<xs:element name="MortgageRank" type="xs:int">
				<xs:annotation>
					<xs:documentation>1 - First 2 - Second 3 - Third etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LoanType" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>0 - Mortgage 
1 - HELOC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExistingBalance" type="xs:decimal" minOccurs="0"/>
			<xs:element name="Amount" type="xs:decimal"/>
			<xs:element name="TotalAmount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Amount + Premium</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Term" type="xs:int"/>
			<xs:element name="Amortization" type="xs:int"/>
			<xs:element name="ClosingDate" type="xs:date"/>
			<xs:element name="IAD" type="xs:date">
				<xs:annotation>
					<xs:documentation>Interest Adjustment Date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentTermType" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>Enum: 
0 - Closed,
1 - Open, 
2 - Convertible</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RepaymentFrequency" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>Enum: 
0 Monthly
1 Semi Monthly
2 Biweekly
3 Accelerated Biweekly
4 Weekly
5 Accelerated Weekly</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RepaymentAmount" type="xs:decimal" minOccurs="0"/>
			<xs:element name="RepaymentType" type="XSEnumCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>0 Blended_PI
1 Interest Plus Fixed Principal
2 Interest Only Variable
3 Interest Only Fixed
4 At_Maturity
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InsuranceType" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>Enum: 
0 - None, 
1 - CMHC, 
2 - GE, 
7 - CG, 
8 - Self-Insured
99 - Other</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Premium" type="xs:decimal" minOccurs="0"/>
			<xs:element name="InsuranceFeeAmount" type="xs:decimal" minOccurs="0"/>
			<xs:element name="InsuranceIncluded" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Rate">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="NetRate" type="xs:decimal"/>
						<xs:element name="InterestRate" type="xs:decimal" minOccurs="0"/>
						<xs:element name="PostedRate" type="xs:decimal" minOccurs="0"/>
						<xs:element name="RequestedRate" type="xs:decimal" minOccurs="0"/>
						<xs:element name="PremiumRate" type="xs:decimal" minOccurs="0"/>
						<xs:element name="Discount" type="xs:decimal" minOccurs="0"/>
						<xs:element name="BrokerBuyDown" type="xs:decimal" minOccurs="0"/>
						<xs:element name="CustomerBuyDown" type="xs:decimal" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="InterestCompound" type="XSEnumCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Enum:
0 - Semi Annually
1 - Monthly
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InterestType" type="XSEnumCodeType">
				<xs:annotation>
					<xs:documentation>Enum:
1 - Fixed
2 - Adjustable
3 - Buydown
4 - VRM
6 - Capped Var</xs:documentation>
